{
   "env": {
      "browser": true,
      "node": true
   },
   "rules": {
      "indent": ["error", 3, { "SwitchCase": 1}],
      "react/jsx-indent": ["error", 3],
      "react/jsx-indent-props": ["error", 3],
      "react/jsx-filename-extension": "off",
      "implicit-arrow-linebreak": ["off"]
   },
   "extends": [
      "airbnb"
   ],
   "plugins": ["react"]
}
